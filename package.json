  "files": [
    "build/",
    "src/",
    "index.js",
    "yarn.lock"
  ],
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.35",
    "babel-polyfill": "^6.26.0",
    "callsite": "^1.0.0",
    "lodash": "^4.17.5",
    "require-like": "^0.1.2",
    "resolve": "^1.5.0",
    "semver": "^5.4.1"
  },
  "description": "NodeJs module loading with an asynchronous flavour",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "gulp-babel": "^7.0.0",
    "gulp-commonjs-browser-wrap": "^0.7.1",
    "gulp-concat": "^2.6.1",
    "gulp-mocha": "^4.3.1",
    "jsdoc-api": "^4.0.0",
    "mocha": "^4.0.1",
    "vinyl-commonjs-dependencies": "^0.7.6"
  },
  "directories": {},
  "homepage": "https://github.com/ssimpo/require-extra#readme",
  "keywords": [
    "require",
    "async",
    "promises",
    "module",
    "bluebird"
  ],
  "license": "MIT",
  "main": "./index.js",
  "maintainers": [
    {
      "email": "me@simpo.org",
      "name": "simpo"
    }
  ],
  "name": "require-extra",
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssimpo/require-extra.git"
  },
  "gulp": {
    "tests": "./test",
    "build": "./build",
    "main": "./src/index.js",
    "mocha": {
      "gobals": [
        "__cwd",
        "gulpSettings"
      ]
    },
    "nodeTargets": [
      "7.0.0",
      "6.5.0",
      "6.0.0",
      "5.0.0",
      "4.0.0"
    ]
  },
  "config": {
    "extensions": [
      ".js",
      ".json",
      ".node"
    ],
    "moduleDirectory": "node_modules",
    "mergeImports": false,
    "useSyncRequire": false
  },
  "scripts": {
    "test": "./gulpfile.js test"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      "transform-runtime",
      "syntax-async-functions",
      "syntax-async-generators",
      "transform-object-rest-spread"
    ]
  },
  "engines": ">=4.0.0",
  "version": "1.5.8"
}
